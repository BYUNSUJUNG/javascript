  <script>  
    var fruits=["Banana","Orange","Apple","Mango"]; 
    console.log(fruits.join("*")); 
    fruits.pop();  
    console.long(fruits);
    fruits.push("Kiwi");  
    console.log(fruits);  
    var shifted_val = fruits.shift();
    console.log("shifted_val --->", shifted_val); 
    var unshifted_val = fruits.unshift("Lemon");  
    console.log("unshifted_val --->", unshifted_val);  
    console.splice(2,0,"banana","pineApple");     
    console.log(fruits);   
    var sliced_val = fruits.slice(1) 
    console.log("sliced_val --->",sliced_val); 
    var sliced_val = fruits.slice(1, 3)  
    console.log("sliced_val --->",sliced_val); 
    var tostring = fruits.toString();  
    console.log(tostring);          
  </script>
  

  <button onclick="myFunction()">확인</button>
  <p id="demo"></p>
  
  <script>  
    var fruit = ["Banana", "Orange", "Apple", "Mango"];
    
    fruit.sort(); 
    console.log(fruits);
    
    fruits.reverse();
    console.log('reverse order : ', fruits); 
    
    var points1 = [40, 30, 15, 25, 10];
    var points2 = [40, 30, 15, 25, 10];
    var points3 = [40, 30, 15, 25, 10];
    
    points1.sort(); 
    console.log("1st sort: ", points1);
    
    points2.sort(function(a,b) { 
    return a - b 
    }); 
    console.log("second sort: ", points2); 
    
    points3.sort(funcion(a,b) {
    return b - a 
    }); 
    console.log("third sort: ",points3); 
    
  </script>
  

<!DOCTYPE html><html><head>    <meta charset="utf-8" /></head><body>    <script>        var user = {          name: "Tim",          age: 23,          sayHello:function() {            return "Hello, I am"+this.name;          }                };                var score = new Object();        score['jj'] = 100;        score['kiti'] = 96;        score.kate = 77;                console.log(score);        for(var key in score) {            console.log(key, ':', score[key]);                    }        console.log(user.sayHello());                    </script></body>    </html>
